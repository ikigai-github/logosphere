{
  "root": "libs/readers",
  "sourceRoot": "libs/readers/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/readers/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/readers"],
      "options": {
        "jestConfig": "libs/readers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:shared"]
}
