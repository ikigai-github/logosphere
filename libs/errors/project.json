{
  "sourceRoot": "libs/errors/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/errors",
        "main": "libs/errors/src/index.ts",
        "tsConfig": "libs/errors/tsconfig.lib.json",
        "assets": ["libs/errors/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/errors/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/errors"],
      "options": {
        "jestConfig": "libs/errors/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    },
    "deploy": {
      "builder": "ngx-deploy-npm:deploy",
      "options": {
        "access": "restricted",
        "buildTarget": "production"
      }
    }
  },
  "tags": ["scope:shared", "type:lib", "kind:support"]
}
