{
  "root": "libs/services/fluree",
  "sourceRoot": "libs/services/fluree/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/services/fluree",
        "main": "libs/services/fluree/src/index.ts",
        "tsConfig": "libs/services/fluree/tsconfig.lib.json",
        "assets": ["libs/services/fluree/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/services/fluree/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/services/fluree"],
      "options": {
        "jestConfig": "libs/services/fluree/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:shared"]
}
