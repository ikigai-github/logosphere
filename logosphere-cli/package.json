{
  "name": "@logosphere/cli",
  "version": "0.0.1",
  "description": "Logoshere SDK CLI",
  "author": "Ikigai Technologies",
  "private": true,
  "license": "MIT",
  "bin": {
    "logos": "bin/logos.ts"
  },
  "bugs": {
    "url": "https://github.com/ikigai-github/logosphere/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikigai-github/logosphere.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "gulp clean:bundle",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint '{lib,commands,actions}/**/*.ts' --fix",
    "start": "node bin/logos.ts",
    "prepack": "pnpm run build",
    "prepublish:next": "pnpm run build",
    "publish:next": "pnpm publish --access public --tag next",
    "prepublish:npm": "pnpm run build",
    "publish:npm": "pnpm publish --access public",
    "test": "jest --config test/jest-config.json",
    "test:dev": "pnpm run clean && jest --config test/jest-config.json --watchAll",
    "prerelease": "pnpm run build",
    "release": "release-it"
  },
  "dependencies": {
    "@angular-devkit/core": "13.1.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "chalk": "3.0.0",
    "cli-table3": "0.6.0",
    "commander": "4.1.1",
    "inquirer": "7.3.3",
    "pascal-case": "3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "release-it": "14.11.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "5.65.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
